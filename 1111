-- =============================================
-- Initialize System
-- =============================================
local function Init()
    -- Make window draggable
    MakeDraggable(Header, MainContainer)
    
    -- Button events
    MinimizeButton.MouseButton1Click:Connect(ToggleVisibility)
    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
    MiniButton.MouseButton1Click:Connect(ToggleVisibility)
    
    -- Create tab contents
    CreateAimbotContent()
    CreateVisualsContent()
    CreatePlayerContent()
    CreateWorldContent()
    CreateMiscContent()
    CreateCreditsContent()
    
    -- Initialize systems
    StartAimbot()
    UpdateWorld()
    
    -- Keybinds for aimbot
    UserInputService.InputBegan:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseButton2 then
            Aimbot.Active = Aimbot.Enabled
        end
    end)
    
    UserInputService.InputEnded:Connect(function(Input)
        if Input.UserInputType == Enum.UserInputType.MouseButton2 then
            Aimbot.Active = false
        end
    end)
    
    -- Auto-attach ESP to players
    Players.PlayerAdded:Connect(function(player)
        if ESP.Enabled then
            CreateESP(player)
        end
    end)
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and ESP.Enabled then
            CreateESP(player)
        end
    end
    
    -- Button hover effects
    local function CreateButtonEffect(button)
        button.MouseEnter:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = button.BackgroundColor3 + Color3.fromRGB(20, 20, 20)}):Play()
        end)
        
        button.MouseLeave:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = button.BackgroundColor3 - Color3.fromRGB(20, 20, 20)}):Play()
        end)
    end
    
    -- Apply effects to all buttons
    for _, tabButton in pairs(TabButtons) do
        CreateButtonEffect(tabButton)
    end
    
    CreateButtonEffect(MinimizeButton)
    CreateButtonEffect(CloseButton)
    CreateButtonEffect(MiniButton)
    
    -- Background animation
    spawn(function()
        while ScreenGui.Parent do
            BackgroundGradient.Rotation = BackgroundGradient.Rotation + 0.3
            wait(0.1)
        end
    end)
    
    UpdateStatus("VEX AIMBOT v1.0 (BETA) Loaded Successfully!")
end

-- Start execution
Init()
